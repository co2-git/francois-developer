section.tab-pane#home.active
  header.hero-unit
    h1 Assets
    p

  div.input-append
    input.span2(type="text", data-ng-model="f.asset", autofocus)
    button.btn Search
  table.table.table-striped.table-hover
    thead
      tr
        th Assets
        td Category
        td Years of experience
    tbody
      tr(data-ng-repeat="asset in assets | filter:f.asset | orderBy:name ")
        td
          a(href="/#/assets/{{asset.name}}", data-ng-bind="asset.name")
        td(data-ng-bind="asset.category")
        td(data-ng-bind="asset.experience")